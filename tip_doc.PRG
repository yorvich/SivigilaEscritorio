PARAMETERS tdc
NTD=''
DO case
   CASE tdc='CC'
      NTD='CÉDULA DE CIUDADANÍA'
   CASE tdc='RC'
      NTD='REGISTRO CIVIL'
   CASE tdc='TI'
      NTD='TARJETA DE IDENTIDAD'
   CASE tdc='CE'
      NTD='CÉDULA DE EXTRANJERÍA'
   CASE tdc='PA'
      NTD='PASAPORTE'
   CASE tdc='MS'
      NTD='MENOR SIN IDENTIFICACIÓN'
   CASE tdc='AS'
      NTD='ADULTO SIN IDENTIFICACIÓN'
ENDCASE
RETURN NTD
